<tr id="<%= Storytime.user_class_underscore.to_s %>_<%= user.id %>">
  <td><%= user.email %></td>
  <td><%= user.storytime_name %></td>
  <td><%= user.storytime_role.name.humanize if user.storytime_role %></td>
  <td class='right'>
    <div class="btn-group">
      <%= link_to t('dashboard.users.edit_user_button'), url_for(:controller => 'dashboard/users', :action => 'edit', :id => user.id), class: "btn btn-xs btn-default" %>
      <%= delete_resource_link user, url_for(:controller => 'dashboard/users', :action => 'destroy', :id => user.id) unless user == current_user %>
    </div>
  </td>
</tr>